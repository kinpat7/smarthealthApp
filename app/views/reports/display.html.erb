<table class="table">
               <thead>
                <tr>
                  <th>Date</th>
                  <th>Time</th>
                  <th >Patient</th>
                  <th >Doctor</th>
                  <th >Clinic</th>
                  <th >Details</th>
                  <th >Actions</th>
                </tr>
              </thead>

              <tbody>
                <% @displayresult.each do |checkup| %>
                  <tr>
                    <td><%= checkup.date %></td>
                    <td><%= checkup.time.strftime("%I:%M%p") %></td>
                  <td><%= Patient.find(checkup['patient_id'])['patient_name'] %></td>
                <td><%= Doctor.find(checkup['doctor_id'])['doctor_name'] %></td>
                 <td><%= Clinic.find(checkup['clinic_id'])['clinic_name'] %></td>

                     <td><%= checkup.summary %></td>
                    <td><button type="button" class: "btn btn-sm-primary"<%= link_to 'Show', checkups_show_path %></button>
                    <button type="button" class: "btn btn-sm-primary"><%= link_to 'Edit', checkups_edit_path(checkup) %></button>
                  </td>
                  </tr>
                <% end %>

              </tbody>
            </table>
